{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["DOM", "ESNext"],
		"jsx": "react-jsx",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["vite.config.ts", "types/*"],
	"exclude": ["node_modules"]
}
